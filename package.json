    "morgan": "~1.7.0",
    "node-sass-middleware": "0.8.0",
    "request": "^2.75.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "css-loader": "^0.25.0",
